{
    "git.autofetch": true,
    "gitdoc.enabled": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.autoPushDelay": 0,
    "gitdoc.commitMessageFormat": "ccc, LLL d, y, h:mm a '+00:00'",
    "flake8.args": [
        "--max-line-length=120",
    ],
    "pylint.args": [
        "--disable=E0401"
    ],
    "mypy-type-checker.args": [
        "var-annotated"
    ],
    "files.insertFinalNewline": true
}
